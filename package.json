{
  "name": "g2g",
  "version": "0.0.0-dev",
  "description": "Marketplace for gamer-to-gamer trades of digital goods",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "analyze": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "build:config": "firebase-ci createConfig",
    "deploy": "firebase-ci deploy -s -i",
    "functions:start": "npm run start --prefix functions",
    "functions:serve": "npm run serve --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "npm run test --prefix functions",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "npm run lint",
    "cm": "git-cz",
    "semantic-release": "semantic-release -d --no-ci --debug"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raulingg/g2g.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "raulingg (https://github.com/raulingg)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "cuid": "^2.1.4",
    "firebase": "^5.6.0",
    "formik": "^1.4.2",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^8.0.3",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.5.3",
    "react-image-crop": "^6.0.12",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.9",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-firestore": "^0.6.0",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.2.0",
    "yup": "^0.26.7"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.9",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^10.0.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.11.1",
    "firebase-ci": "^0.5.2",
    "firebase-tools": "^6.1.2",
    "husky": "^1.2.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.2",
    "react-scripts": "2.1.1",
    "react-test-renderer": "^16.7.0",
    "semantic-release": "^15.13.3",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
