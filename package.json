{
  "name": "g2g",
  "version": "0.0.0-dev",
  "description": "Marketplace for gamer-to-gamer trades of digital goods",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "analyze": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json",
    "build:config": "firebase-ci createConfig",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "deploy": "firebase-ci deploy -s -i",
    "functions:start": "npm run start --prefix functions",
    "functions:serve": "npm run serve --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "npm run test --prefix functions",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:report": "cypress run --record false",
    "cypress:report:generate": "mochawesome-merge --reportDir cypress/results > mochawesome.json && marge mochawesome.json",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:baseUrl": "export CYPRESS_BASE_URL=http://localhost:8000 && cypress run",
    "serve": "ws -d build --spa index.html --port 8000 --hostname localhost",
    "e2e:ci": "run-p --race serve cypress:run:baseUrl",
    "e2e:ci:report": "run-p --race serve \"cypress:run:baseUrl --record false\"",
    "e2e:ci:parallel": "run-p --race serve \"cypress:run:baseUrl --parallel\"",
    "e2e:ci:record": "run-p --race serve \"cypress:run:baseUrl --record\"",
    "e2e:ci:record:parallel": "run-p --race serve \"cypress:run:baseUrl --parallel --record\"",
    "e2e:ci:chrome": "run-p --race serve \"cypress:run:baseUrl --browser chrome\"",
    "e2e:ci:headed": "run-p --race serve \"cypress:run:baseUrl --headed\"",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "npm run lint",
    "cm": "git-cz",
    "semantic-release": "semantic-release -d --no-ci --debug"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raulingg/g2g.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "raulingg (https://github.com/raulingg)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "cuid": "^2.1.4",
    "firebase": "^5.9.3",
    "formik": "^1.4.2",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^10.0.5",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.5.3",
    "react-image-crop": "^6.0.17",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.2.6",
    "react-router-dom": "^5.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-firestore": "^0.7.0",
    "redux-form": "^8.1.0",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.2.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.9",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^10.0.0",
    "commitizen": "^3.0.5",
    "cypress": "^3.2.0",
    "cypress-firebase": "^0.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "firebase-ci": "^0.5.2",
    "firebase-tools": "^6.1.2",
    "husky": "^1.2.0",
    "local-web-server": "^2.6.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "mochawesome-merge": "^2.0.0",
    "mochawesome-report-generator": "^3.1.5",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.2",
    "react-scripts": "2.1.8",
    "react-test-renderer": "^16.7.0",
    "semantic-release": "^15.13.3",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
